<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

  <class name="mjs.model.FamilyMember" table="family_members">
    <id name="family_member_pk" column="family_member_pk" type="int">
      <generator class="increment"/>
    </id>
    <property name="fname" type="string" not-null="true"/>
    <property name="lname" type="string" not-null="true"/>
    <property name="description" type="string" not-null="false"/>
    <property name="dob" type="date" not-null="false"/>
  </class>
</hibernate-mapping>
